<html>
  <head>
    <title>Histories | Update</title>
  </head>
  <body>
    <form method="POST" action="/histories/update/<%= histories.id %>">
      <label for="tanggal_main">tanggal_main</label>
      <br>
      <input type="string" name="tanggal_main” value="<%= histories.tanggal_main %>"/>
      <br>

      <label for="hasil">hasil</label>
      <br>
      <input type="string" name="hasil" value="<%= histories.hasil %>"/>
      <br>

      <button type="submit">Submit</button>
    </form>
  </body>
</html>